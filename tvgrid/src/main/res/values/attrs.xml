<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TVGridView">
        <attr name="tvg_selectorPosition" format="enum" >
            <enum name="over" value="0"/>
            <enum name="under" value="1"/>
        </attr>
        <attr name="tvg_strokePosition" format="enum">
            <enum name="inside" value="0"/>
            <enum name="center" value="1"/>
            <enum name="outside" value="2"/>
        </attr>
        <attr name="tvg_selectorShape" format="enum">
            <enum name="rectangle" value="0"/>
            <enum name="circle" value="1"/>
        </attr>
        <attr name="tvg_animateSelectorChanges" format="boolean"/>
        <attr name="tvg_filled" format="boolean"/>
        <attr name="tvg_fillAlpha" format="float"/>
        <attr name="tvg_fillAlphaSelected" format="float"/>
        <attr name="tvg_fillColor" format="color"/>
        <attr name="tvg_fillColorSelected" format="color"/>
        <attr name="tvg_strokeColor" format="color"/>
        <attr name="tvg_strokeColorSelected" format="color"/>
        <attr name="tvg_strokeWidth" format="dimension"/>
        <attr name="tvg_cornerRadius" format="dimension"/>
        <attr name="tvg_marginLeft" format="dimension"/>
        <attr name="tvg_marginTop" format="dimension"/>
        <attr name="tvg_marginRight" format="dimension"/>
        <attr name="tvg_marginBottom" format="dimension"/>
        <attr name="tvg_spacingLeft" format="dimension"/>
        <attr name="tvg_spacingTop" format="dimension"/>
        <attr name="tvg_spacingRight" format="dimension"/>
        <attr name="tvg_spacingBottom" format="dimension"/>
    </declare-styleable>
</resources>